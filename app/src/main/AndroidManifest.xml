<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.doctorpescribed">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".doctor_reg"></activity>
        <activity android:name=".dr_password_reset" />
        <activity
            android:name=".patient_nav"
            android:label="@string/title_activity_patient_nav"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".doctor_nav"
            android:label="@string/title_activity_doctor_nav"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".patientlogin"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".doctorlogin"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".ui.login.patientlogins"
            android:label="@string/title_activity_patientlogins" />
        <activity
            android:name=".ui.login.doctorlogin"
            android:label="@string/title_activity_doctorlogin" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>